{
  "name": "coconutculture",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "node-sass-chokidar": "^1.4.0",
    "nodemon": "^2.0.3",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "shippo": "^1.5.0",
    "stripe": "^8.47.0",
    "superagent": "^5.2.2",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn --cwd client/app build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "nodemon server.js",
    "client": "concurrently \"yarn --cwd client/app start\" \"yarn --cwd client/app watch-css\"",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "prod": "concurrently \"yarn run server\" \"yarn --cwd client/app start\"",
    "sass": "yarn --cwd client/app watch-css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "yarn-upgrade-all": "^0.5.2"
  }
}
